  "license": "MIT",
  "devDependencies": {
    "@types/bunyan": "^1.8.5",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/dotenv": "^6.1.0",
    "@types/jest": "^22.2.3",
    "@types/supertest": "^2.0.7",
    "jest": "^23.0.1",
    "supertest": "^3.3.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.12.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "bunyan-logstash-tcp": "^1.0.0",
    "continuation-local-storage": "^3.2.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "inversify": "^4.13.0",
    "inversify-express-utils": "^6.2.0",
    "moment": "^2.22.1",
    "prom-client": "^11.0.0",
    "reflect-metadata": "^0.1.12",
    "uuid": "^3.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/typings"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
